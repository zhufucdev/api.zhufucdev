{
    "framework": "nextjs",
    "rewrites": [
        {
            "source": "/v1/:match*",
            "destination": "/api/:match*"
        },
        {
            "source": "/:match*",
            "destination": "/api/:match*"
        },
        {
            "source": "/",
            "destination": "/api/index"
        }
    ]
}
